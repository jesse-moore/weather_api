{
  "name": "azure-functions-template",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/functions/httpTrigger.js",
  "scripts": {
    "build": "npm run clean && npx rollup --config && npm run zip",
    "clean": "rimraf .rollup && rimraf ./build && rimraf ./dist",
    "deploy": "node deploy.js",
    "watch": "tsc -w",
    "prestart": "npm run build && func extensions install",
    "start": "npm-run-all --parallel start:host watch",
    "start:host": "func start",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "zip": "node archiver.js"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0-alpha.9"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/chai": "^4.3.8",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "archiver": "^6.0.1",
    "azure-functions-core-tools": "^4.x",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.1.3",
    "tslib": "^2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.0"
  }
}
